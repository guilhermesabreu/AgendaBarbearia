<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Schedule Cut</title>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <link href="~/Content/Style.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-united.css" rel="stylesheet" type="text/css" />
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600' rel='stylesheet' type='text/css'>
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.1.1/css/font-awesome.css" rel="stylesheet">
</head>

<body>
    @RenderBody()
    <section class="footer-site" style="margin-bottom:-100%;margin-right:0%;">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="container body-content">
                        <hr style="height:1px;background-color:black" />
                        <footer>
                            <p>&copy; @DateTime.Now.Year - All CopyRight Reserved</p>
                            <p>Software Criado pela Abreu Systems</p>
                        </footer>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="~/Scripts/handlerHeader.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>